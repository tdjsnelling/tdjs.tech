{
  "extends": [ "standard", "standard-react" ],

  "rules": {
    "jsx-quotes": [2, "prefer-double"],
    "object-curly-spacing": [ "error", "always" ],
    "array-bracket-spacing": [ "error", "always" ],
    "space-before-function-paren": 0,
    "react/prop-types": 0,
    "react/display-name": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}
